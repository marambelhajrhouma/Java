package DS2019;

public class BoiteCourrier {
	public static void main(String[] args) {
	Courrier[] tab= new Courrier[5];
	//
	tab[0] = new Lettre(true, "Address1", 150, "A4");
    tab[1]= new Colis(false, "Address2", 500.0, 2.0);
    //
    for(int i=0;i<tab.length;i++) {
    	tab[i].decrire();
    }
	}	
}
