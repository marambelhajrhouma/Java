package TP5_essai;
import java.util.Scanner;
public class Tab {
	private static final int nmax=40;
	
	//
	public static int getNmax() {
		return nmax;
	}
	
	//
	static void remplir(float t[],Scanner scanner) {
		
		for(int i=0;i<t.length;i++) {
			System.out.println("entrer une val dans le tableau ");
			t[i]=scanner.nextFloat();
		}
	}
	/*
	static float somme(float t[]) {
		float s=0;
		for(int i=0;i<t.length;i++) {
			s+=t[i];
		}
		return s;
	}
	
	static float[] additionner(float t1[],float t2[]) {
		int i=0;
		int taille=Math.min(t1.length,t2.length);
		float[] tres=new float[taille];
		while(i<t1.length && i<t2.length) {
			tres[i]=t1[i]-t2[i];
			i++;
		}
		return tres;
	}
	
	static float[] produit(float x, float t[]) {
		float[] res=new float[t.length];
		for(int i=0;i<t.length;i++) {
			res[i]=t[i]*x;
		}
		return res;
	}
	*/
	static void lister(float t[]) {
		for(int i=0;i<t.length;i++) {
			System.out.println(t[i] +" ");
		}
		System.out.println();
	}
	
	static float[] moyenne(float m1,float m2,float t[]) {
		float[] tt= new float[t.length];
		for(int i=0;i<t.length;i++) {
			tt[i]=m1+m2*2;
		}
		return tt;
	}
	/*
	static float[] moyenne-etudiants(){
